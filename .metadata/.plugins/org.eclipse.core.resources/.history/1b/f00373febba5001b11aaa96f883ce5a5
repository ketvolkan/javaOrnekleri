package classornek;

public class Main {

	public static void main(String[] args) {
		ogrenci ogr = new ogrenci("Volkan","Ket","Aydýn",19);
		ogretmen ogrtmn = new ogretmen("Engin","Demiroð","Bilinmiyor",30);
		ogrtmn.katilma(ogr);
		ogrtmn.puanDegistir(ogr, 30);
		ogr.puanGoster();
		ogrtmn.puanDegistir(ogr, -20);
		ogr.puanGoster();
	}

}
