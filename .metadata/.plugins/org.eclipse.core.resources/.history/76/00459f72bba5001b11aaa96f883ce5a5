package classornek;

public class ogrenci 
{
	String Ad,Soyad,Memleket;
	Byte Yas;
	static Byte puan=50;	
	public ogrenci(String ad,String soyad,String memleket,Byte yas)
	{
		Ad = ad;
		Soyad = soyad;
		Memleket = memleket;
		Yas = yas;
	}
	public static void puanGoster() 
	{
		System.out.println(puan);
	}
}

