package classornek;

public class Main {

	public static void main(String[] args) {
		ogrenci volkan = new ogrenci("Volkan","Ket","Aydýn",19);
		ogretmen engin = new ogretmen("Engin","Demiroð","Bilinmiyor",30);
		engin.katilma(volkan);
		engin.puanDegistir(volkan, 30);
		volkan.puanGoster();
		engin.puanDegistir(volkan, -20);
		volkan.puanGoster();
	}

}
