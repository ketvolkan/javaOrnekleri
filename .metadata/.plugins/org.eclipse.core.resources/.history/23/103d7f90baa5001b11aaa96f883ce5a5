package classornek;

public class ogrenci {
	String Ad,Soyad,Memleket;
	Byte Yas;	
	public ogrenci(String ad,String soyad,String memleket,Byte yas)
	{
		Ad = ad;
		Soyad = soyad;
		Memleket = memleket;
		Yas = yas;
	}

}
public class ogretmen {

	public static void katilma(ogrenci katilan)
	{
		System.out.println(katilan.Ad+" ");
	}
}
